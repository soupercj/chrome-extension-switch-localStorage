{
	"manifest_version": 2,
	"name": "Switch localStorage",
	"description": "switch localStorage for developing",
	"version": "0.2",
	"icons": {
		"16": "images/switch-16.png",
    "48": "images/switch-48.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"storage"
	],
  "content_scripts": [
    {
      "matches": ["http://home.zaobang.net/*", "http://localhost:8888/*"],
      "js": ["js/jquery-3.2.1.min.js", "js/content.js"]
    }
  ]
}
