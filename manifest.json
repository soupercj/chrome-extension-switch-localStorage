{
	"manifest_version": 2,
	"name": "DevAccountSwitch",
	"description": "switch account for developing",
	"version": "0.1",
	"icons": {
		"16": "images/16.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "images/16.png"
		},
		"default_title": "show weather",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"storage"
	],
  "content_scripts": [
    {
      "matches": ["http://home.zaobang.net/*", "http://localhost:8888/*"],
      "js": ["js/jquery-3.2.1.min.js", "js/content.js"]
    }
  ]
}
